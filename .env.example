# ===========================================
# Instituto UP — Painel Administrativo
# ===========================================
# Copie este arquivo para .env.local e preencha os valores.
# cp .env.example .env.local

# Connection string do PostgreSQL
DATABASE_URL=postgres://usuario:senha@host:5432/banco

# Habilitar SSL na conexão com o banco (true para provedores como Aiven, Neon, Vercel Postgres)
PG_SSL=true

# Token de autenticação do painel (compartilhado entre usuários)
DASHBOARD_TOKEN=seu_token_seguro_aqui

# Configuração de treinamentos ativos (JSON)
# Exemplo: [{"id":"2026-03-01","label":"Turma Março 2026","startsAt":"2026-03-01T09:00:00Z"}]
TRAINING_CONFIG=[]
